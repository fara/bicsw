<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
    <class name="ar.com.bic.domain.Atributo, ar.com.bic.domain" table="atributo">
        <id name="id" column="id" type="long" access="field" > 
            <generator class="native" /> 
        </id> 
        <property name="nombre" column="nombre" type="String" access="field"/> 
        <property name="campoId" column="campoId" type="String" access="field"/> 
        <property name="campoDescripcion" column="campoDescripcion" type="String" access="field"/>		
		<property name="tablaLookup" column="tablaLookup" type="String" access="field"/>
		<many-to-one name="proyecto" class="ar.com.bic.domain.Proyecto, ar.com.bic.domain" column="proyecto_id" not-null="true" access="field"/>
    </class>
</hibernate-mapping>