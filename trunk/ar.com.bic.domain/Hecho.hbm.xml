<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
    <class name="ar.com.bic.domain.Hecho, ar.com.bic.domain" table="hecho">
        <id name="id" column="id" type="long" access="field" > 
            <generator class="native" /> 
        </id> 
        <property name="nombre" column="nombre" type="String" access="field"/> 
		<many-to-one name="columna" class="ar.com.bic.domain.catalogo.Columna, ar.com.bic.domain" column="columna_id" not-null="true" access="field" insert="true" cascade="all" />
		<many-to-one name="proyecto" class="ar.com.bic.domain.Proyecto, ar.com.bic.domain" column="proyecto_id" not-null="true" access="field"/>
    </class>
</hibernate-mapping>