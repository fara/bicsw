<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
    <class name="ar.com.bic.domain.catalogo.Columna, ar.com.bic.domain" table="columna">
        <id name="id" column="id" type="long" access="field" > 
            <generator class="native" /> 
        </id> 
        <property name="nombre" column="nombre" type="String" access="field"/>
        <property name="tipo" column="tipo" type="String" access="field"/>
		<many-to-one name="tabla" class="ar.com.bic.domain.Proyecto, ar.com.bic.domain" column="tabla_id" not-null="true" access="field"/>
    </class>
</hibernate-mapping>