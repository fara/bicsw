<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">

	<description>
		Configuracion de los DAOs
	</description>

	<import resource="spring-orm.xml"/>

	<!-- DAOs -->
	<object id="genericDAO" type="ar.com.bic.dao.GenericDAO, ar.com.bic.dao" >
		<property name="SessionFactory" ref="sessionFactory"/>	
	</object>

	<object id="usuarioDAO" type="ar.com.bic.dao.UsuarioDAO, ar.com.bic.dao" >
		<property name="SessionFactory" ref="sessionFactory"/>	
	</object>

	<object id="proyectoDAO" type="ar.com.bic.dao.ProyectoDAO, ar.com.bic.dao" >
		<property name="SessionFactory" ref="sessionFactory"/>	
	</object>
	
</objects>
